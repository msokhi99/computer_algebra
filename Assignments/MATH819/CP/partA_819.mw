<Group labelreference="L410" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">DIVALGO := proc(f::polynom,F::list(polynom),X::list(symbol),ord::symbol)
local Q,R,P,L,i,j,LTP,sizeF,flag:

sizeF := nops(F):
Q := Array(1..sizeF):
R := 0: (* Remainder Column. *)
P := f: (* Intermediate Dividend. *)
L := [seq(LT(F[j],X,ord),j=1..sizeF)]:

while P&lt;&gt;0 do:
    flag := false:
    LTP := LT(P,X,ord):
    for i from 1 to sizeF while not flag do
        if L[i]&lt;&gt;0 then:
            if divide(LTP,L[i],'q') then
                Q[i] := Q[i]+q:
                P := P-(expand(q*F[i])):
                flag := true:
            fi:
        fi:
    od:
    if flag=false then:
        R := R+LTP:
        P := P-LTP:
    fi:
od:

return R:
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L411" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">LT := proc(f::polynom,X::list(symbol),ord::symbol)
