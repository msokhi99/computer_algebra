<Group labelreference="L45" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MAPLE2SDMP := proc(a,X::list(name))

local C,M,n,t,A,E,i,m:

if not type(a,polynom(rational,X)) then
    error &quot;BAD INPUT&quot;
fi:

C := [coeffs(a,X,'M')]:
M := [M]:
n := nops(X):
t := nops(M):
E := [seq([seq(degree(m,X[i]),i=1..n)],m in M)]:
A := [seq([C[i],E[i]],i=1..t)]:

sort(A,proc(x,y) GREATER(x[2],y[2]) end):
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L43" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">SDMP2MAPLE := proc(A::list([rational,list(nonnegint)]),X::list(name))

local t,i,n:

n := nops(X):
add(t[1]*mul(X[i]^t[2][i],i=1..n),t in A):
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L44" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GREATER := proc(a::list(nonnegint),b::list(nonnegint))

local dega,degb,n,i,z:
global COMP:

COMP := COMP+1:
n := nops(a):
if nops(b)&lt;&gt;n then:
    error &quot;EXP VECTORS OF DIFF SIZE&quot;:
fi:

dega := add(z,z in a):
degb := add(z,z in b):

if dega&gt;degb then:
    return true:
elif dega&lt;degb then:
    return false:
fi:

for i to nops(a) do:
    if a[i]&gt;b[i] then:
        return true:
    elif a[i]&lt;b[i] then:
        return false:
    fi:
od:
false:
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L54" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">ADDPOL := proc(f::list([rational,list(nonnegint)]),g::list([rational,list(nonnegint)]))

local m,n,h,i,j,k,c,tempOne,tempTwo,addOne,addTwo,x,ratio:
m := nops(f):
n := nops(g):
h := Array(1..m+n):
i := 1:
j := 1:
k := 0:

while i&lt;=m and j&lt;=n do:
    if f[i][2]=g[j][2] then:
        c := f[i][1]+g[j][1]:
        if c&lt;&gt;0 then:
            k++:
            h[k] := [c,f[i][2]]:
        fi:
        i++:
        j++:
    elif GREATER(f[i][2],g[j][2])=true then:
        k++:
        h[k] := f[i]:
        i++:
    else:
        k++:
        h[k] := g[j]:
        j++:
    fi:
od:

while i&lt;=m do:
    k++:
    h[k] := f[i]:
    i++:
od:

while j&lt;=n do:
    k++:
    h[k] := g[j]:
    j++:
od:

(* Converting TABLE to a LIST *)
h := [seq(h[i],i=1..k)]:

return (h):
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L62" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MULPOL := proc(f::list([rational,list(nonnegint)]),g::list([rational,list(nonnegint)]))

local h,s,t,T,i,termsG:
global CMUL:

h := []:
termsG := nops(g):
for s in f do:
    T := [seq([s[1]*t[1],s[2]+t[2]],t in g)]:
    CMUL := CMUL+termsG:
    h := ADDPOL(h,T):
od:

return h:
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L50" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MULPOLDAC := proc(f::list([rational,list(nonnegint)]),g::list([rational,list(nonnegint)]))

local m,n,mid,left,right,res:

m := nops(f):
n := nops(g):

if m=0 or n=0 then:
    return []:
elif m=1 or n=1 then:
    return MULPOL(f,g):
fi:

if m&gt;=n then:
    mid := floor(m/2):
    left := MULPOLDAC([op(f[1..mid])],g):
    right := MULPOLDAC([op(f[mid+1..m])],g):
else:
    mid := floor(n/2):
