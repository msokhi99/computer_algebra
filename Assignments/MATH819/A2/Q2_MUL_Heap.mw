<Group labelreference="L273" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MAPLE2SDMP := proc(a,X::list(name))

local C,M,n,t,A,E,i,m:

if not type(a,polynom(rational,X)) then
    error &quot;BAD INPUT&quot;
fi:

C := [coeffs(a,X,'M')]:
M := [M]:
n := nops(X):
t := nops(M):
E := [seq([seq(degree(m,X[i]),i=1..n)],m in M)]:
A := [seq([C[i],E[i]],i=1..t)]:

sort(A,proc(x,y) GREATER(x[2],y[2]) end):
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L275" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">SDMP2MAPLE := proc(A::list([rational,list(nonnegint)]),X::list(name))

local t,i,n:

n := nops(X):
add(t[1]*mul(X[i]^t[2][i],i=1..n),t in A):
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L274" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GREATER := proc(a::list(nonnegint),b::list(nonnegint))

local dega,degb,n,i,z:
n := nops(a):
if nops(b)&lt;&gt;n then:
    error &quot;EXP VECTORS OF DIFF SIZE&quot;:
fi:

dega := add(z,z in a):
degb := add(z,z in b):

if dega&gt;degb then:
    return true:
elif dega&lt;degb then:
    return false:
fi:

for i to nops(a) do:
    if a[i]&gt;b[i] then:
        return true:
    elif a[i]&lt;b[i] then:
        return false:
    fi:
od:
false:
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L271" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GRT := proc(a,b)

global monComp:
monComp := monComp+1:

evalb(GREATER(b[3],a[3])):
end proc:</Text-field>
</Input>
</Group>
<Group hide-input="false" labelreference="L272" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">X := [u,v,w,x,y,z]:
a := randpoly(X,degree=10,terms=5000):
