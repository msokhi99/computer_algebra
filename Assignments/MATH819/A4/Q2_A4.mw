<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">GETDEG := proc(paramA::procedure)

local BB,p,r,alpha,m,yK,vK,k,gK:

BB := paramA:
p := 2^62+135:
r := rand(0..p-1):
m := 1:
k := 0:
gK := 0:

while true do:
    alpha := r():
    while (eval(m,x=alpha)) mod p=0 do:
        alpha := r():
        next:
    od:
    yK := BB(alpha):
    vK := ((yK-eval(gK,x=alpha))/eval(m,x=alpha)) mod p:
    
    if vK=0 then:
        return (k-1):
    fi:

    gK := (gK+vK*m) mod p:
    m := (expand(m*(x-alpha))) mod p:
    k := k+1:
od:
end proc:</Text-field>
</Input>
