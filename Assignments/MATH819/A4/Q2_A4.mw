<Group labelreference="L2" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">(* Creates a VanderMonde Matrix of size n. *)

CREATEVANMAT := proc(paramA::posint)

local n,xVals,i,j,vanMat,valIn:
n := paramA:

xVals := [seq(x[i],i=1..n)]:
vanMat := Matrix(n,n):

for i from 1 to n do:
    local k:
    k := 1:
    valIn := xVals[i]:
    for j from 1 to n do:
        if j=1 then:
            vanMat[i,j] := 1:
        else:
            vanMat[i,j] := valIn^k:
            k := k+1:
        fi:
    od:
od:
 
return vanMat:
end proc:</Text-field>
</Input>
</Group>
<Group labelreference="L51" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CREATETOPEMAT := proc(paramA::posint)

local n,X,i,j,mat:

n := paramA:
X := [seq(x[i],i=1..n)]:
mat := Matrix(n,n):

for i from 1 to n do:
    for j from 1 to n do:
        mat[i,j] := X[abs(i-j)+1]:
    od:
od:

return mat:
end proc:</Text-field>
</Input>
</Group>
