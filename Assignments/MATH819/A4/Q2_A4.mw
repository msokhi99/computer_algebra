<Group hide-input="false" labelreference="L17" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">BB := proc(paramA::list(integer),paramB::prime)

local alpha,p,mat,det,res,resL,i,k,temp:

alpha := paramA:
p := paramB:
mat := [bbInput1,bbInput2]:
resL := table():
k := 1:

for i from 1 to nops(mat) do:
    temp := mat[i]:
    det := Determinant(temp):
    res := eval(det,{seq(x[i]=alpha[i],i=1..n)}) mod p:
    resL[k] := [res]:
    k := k+1:
od:

resL := convert(resL,list):
return resL:
end proc:</Text-field>
</Input>
