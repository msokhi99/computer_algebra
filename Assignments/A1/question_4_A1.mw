<Group labelreference="L9" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">swap_num := proc(a::posint, b::posint)

    <Font foreground="[0,255,0]">(* Initializing local variables *)</Font>
    
    local temp_var, A, B; 
    A := a;
    B := b;
    temp_var := A;
    A := B;
    B := temp_var;

    return A,B;
end proc;</Text-field>
</Input>
