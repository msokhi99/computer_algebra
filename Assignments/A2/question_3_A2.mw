<Group hide-input="false" labelreference="L33" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">(* Primitive Euclidiean Algorithm Implementation *)

prim_algo := proc(a::polynom, b::polynom)

local A, B, k, r, g, pr:
A, B, k := a, b, 1:
r[0] := primpart(A,x):
r[1] := primpart(B,x):

while r[k] &lt;&gt; 0 do
    pr := prem(r[k-1],r[k],x):
    r[k+1] := primpart(pr,x):
    k := k+1;
od:

g := r[k-1];
g := -1*expand((gcd(content(a,x),content(b,x))*g));
<Font encoding="UTF-8">return printf(&quot;The GCD using prim E algo is: %a.\134n&quot;, g);
