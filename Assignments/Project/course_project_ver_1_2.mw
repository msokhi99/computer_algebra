<Group labelreference="L63" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CRT := proc(var_A::list, var_B::list, t, q::prime)

local U:local M:local g:local v;local temp:
U := var_A:
M := var_B:

if nops(U) &lt;&gt; nops(M) then
    error &quot;FAIL&quot;:
elif nops(U) = 1 then
    return U[1], M[1]:
fi:

g := Gcdex(M[1], M[2], t, 'a', 'b') mod q:
v := U[1]*b*M[2] + U[2]*a*M[1]:temp := M[1]*M[2] mod q:
v := expand(Rem(v, temp, t) mod q):
  
U := U[3..-1]:
U := [op(U), v]:
M := [op(M), Expand(temp) mod q]:
M := M[3..-1]:
    
return CRT(U, M, t, q):
end proc:</Text-field>
</Input>
</Group>
